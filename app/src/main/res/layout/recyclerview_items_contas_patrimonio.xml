<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardview1"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@drawable/boxrecycler"
    card_view:cardCornerRadius="3dp"
    card_view:cardElevation="3dp"
    card_view:cardMaxElevation="3dp"
    card_view:contentPadding="3dp">


    <RelativeLayout
        android:id="@+id/cdll2"
        android:layout_width="fill_parent"
        android:layout_height="85dp">

        <LinearLayout
            android:id="@+id/cdll_outros"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/VollyNet3workImageView1"
            android:background="@drawable/boxrecycler"
            android:gravity="center_vertical"
            android:orientation="vertical"
            tools:ignore="UnknownId">

            <TextView
                android:id="@+id/descricao_contas_patrimonio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="7dp"
                android:layout_toEndOf="@+id/VollyNetworkImageView1"
                android:text="Nome Evento"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#3c4045"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/id_conta_patrimonio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:visibility="gone" />


        </LinearLayout>

    </RelativeLayout>

</android.support.v7.widget.CardView>